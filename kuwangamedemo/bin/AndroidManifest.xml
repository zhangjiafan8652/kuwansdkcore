<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kkgamedemo6"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="23" />
    <!-- 丫丫玩sdk 必须权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
     <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.DOWNLOAD_WITHOUT_NOTIFICATION" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />
   
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true" />

    
     <instrumentation android:name="android.test.InstrumentationTestRunner"
        android:targetPackage="com.example.kkgamedemo"
        android:label="label" />
    <application
        android:name="com.yayawan.impl.YYApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        
        <uses-library android:name="android.test.runner" />
        
        <activity
            android:name="com.example.kkgamedemo.MainActivity"
            android:screenOrientation="landscape"
            android:configChanges="keyboardHidden|navigation|orientation|screenSize" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

       
        
          <!-- BaseLogin_Activity 登录 -->
        <activity
            android:name="com.kkgame.sdk.login.BaseLogin_Activity"
            android:configChanges="keyboardHidden|navigation|orientation|screenSize"
            android:theme="@android:style/Theme.Dialog" 
            >
          
       </activity>
        <activity
            android:name="com.kkgame.sdk.login.SmallHelpActivity"
          	android:configChanges="keyboardHidden|navigation|orientation|screenSize"
          	android:theme="@android:style/Theme.Translucent" 
           >
        </activity>

        <!-- yayawan_nochangecount设置为true这取消切换账号..除个别特殊游戏外.都不能取消切换账号这个选项 -->
        <meta-data
            android:name="yayawan_nochangecount"
            android:value="false" />
       <activity
            android:name="com.kkgame.sdk.login.AssistantActivity"
            android:configChanges="keyboardHidden|navigation|orientation|screenSize"
             >
        </activity>
     
        
        <activity
            android:name="com.kkgame.sdk.pay.XiaoMipayActivity"
            android:configChanges="keyboardHidden|navigation|orientation|screenSize"
            android:theme="@style/dialogstyle" >
        </activity>
       
       
  
        <!-- 是否为调试模式 -->
        <meta-data
            android:name="isdebug"
            android:value="true" />

  

        <!-- 横竖屏设置 -->
  
		 <meta-data
            android:name="isLandscape"
            android:value="true" />

        <!-- 腾讯广点通上报方案 -->
      
        <meta-data
            android:name="guangdiantong"
            android:value="no" />
        
        
          <meta-data
            android:name="app_id"
            android:value="kk1531772467" /> 
          
        <meta-data
            android:name="qianqi_app_id"
            android:value="kk1129901475" /> 
       
             
      
        <meta-data
            android:name="banhaoxinxi"
            android:value="出版社：发动机是垃圾费多了时间了解放东路卡机发了多少" />
        <meta-data
            android:name="isxiaomi"
            android:value="false" />
        
       <meta-data
            android:name="zoneid"
            android:value="string2" />  
      
          <meta-data
            android:name="sdktype"
            android:value="string0" />  
              <meta-data
            android:name="kefuqq"
            android:value="string123456" />  
        
       
    </application>

</manifest>